<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="PrimaryButtonStyle">
        <item name="backgroundTint">@color/cyan_process</item>
        <item name="background">@drawable/btn_shape</item>
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="white_line">
        <item name="backgroundTint">@color/white</item>
        <item name="background">@drawable/whiteline_shape</item>
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="gradient_style">
        <item name="android:backgroundTintMode">screen</item>
        <item name="background">@drawable/gradient_btn</item>
        <item name="fontFamily">@font/raleway_light</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>